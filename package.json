{
  "name": "architecture",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"typed-scss-modules src/**/*.module.scss --aliasPrefixes.styles src/styles -w --ignoreInitial --nameFormat dashes\"",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "stylelint": "npx stylelint \"**/*.scss\"",
    "stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
    "prettier:fix": "prettier . --write --cache",
    "plop:cc": "plop create-component",
    "check:styles-types": "typed-scss-modules src/**/*.module.scss --listDifferent --aliasPrefixes.styles src/styles --nameFormat dashes",
    "check:typescript": "tsc",
    "generate-scss-types": "typed-scss-modules src/**/*.module.scss --aliasPrefixes.styles src/styles --nameFormat dashes"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.10",
    "@tanstack/react-router": "^1.92.3",
    "@types/node": "^22.10.2",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.4.3",
    "humps": "^2.0.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.1",
    "qs": "^6.13.1",
    "react": "^18.3.1",
    "react-aria-components": "^1.5.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^15.2.0",
    "react-use": "^17.6.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.62.10",
    "@tanstack/router-devtools": "^1.92.3",
    "@types/humps": "^2.0.6",
    "@types/qs": "^6.9.17",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^9.1.1",
    "eslint": "8.45.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "plop": "^4.0.1",
    "prettier": "3.0.3",
    "sass": "1.67.0",
    "stylelint": "^16.11.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "5.0.0",
    "stylelint-semantic-groups": "1.2.1",
    "typed-scss-modules": "8.0.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vite-plugin-svgr": "^4.3.0"
  }
}